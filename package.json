{
  "name": "volar-plugin-ignore-attributes",
  "type": "module",
  "version": "0.2.6",
  "packageManager": "pnpm@10.14.0",
  "description": "A Volar plugin to ignore unocss attributes for JSX.",
  "author": "zhiyuanzmj <zhiyuanzmj@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/zhiyuanzmj",
  "homepage": "https://github.com/zhiyuanzmj/volar-plugin-ignore-attributes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhiyuanzmj/volar-plugin-ignore-attributes.git"
  },
  "bugs": "https://github.com/zhiyuanzmj/volar-plugin-ignore-attributes/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "DEV=true tsdown",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "muggle-string": "^0.4.1",
    "ts-macro": "^0.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.0",
    "@antfu/utils": "^9.2.0",
    "@types/node": "^24.2.1",
    "bumpp": "^10.2.2",
    "eslint": "^9.32.0",
    "lint-staged": "^16.1.5",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "^0.13.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
